{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "dist/main.js" }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://www.harpyjs.org/:path*",
      "permanent": true
    }
  ]
}
