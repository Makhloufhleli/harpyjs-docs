{
  "version": 2,
  "outputDirectory": "dist",
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    },
    {
      "src": "dist/favicon.png",
      "use": "@vercel/static"
    },
    {
      "src": "dist/favicon.svg",
      "use": "@vercel/static"
    },
    {
      "src": "dist/twitter-card.jpg",
      "use": "@vercel/static"
    },
    {
      "src": "dist/manifest.json",
      "use": "@vercel/static"
    },
    {
      "src": "dist/google1398675d6f9b2930.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/favicon.png", "dest": "/dist/favicon.png" },
    { "src": "/favicon.svg", "dest": "/dist/favicon.svg" },
    { "src": "/twitter-card.jpg", "dest": "/dist/twitter-card.jpg" },
    { "src": "/manifest.json", "dest": "/dist/manifest.json" },
    { "src": "/google1398675d6f9b2930.html", "dest": "/dist/google1398675d6f9b2930.html" },
    { "src": "/(.*)", "dest": "/dist/main.js" }
  ]
}